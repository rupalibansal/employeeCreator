{
  "compilerOptions": {
    "target": "es5",
    "module": "es2020",
    "lib": ["dom", "es2015"],
    "jsx": "react", // Add this line
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}